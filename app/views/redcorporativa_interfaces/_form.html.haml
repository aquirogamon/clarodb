= form_for @redcorporativa_interface do |f|
  - if @redcorporativa_interface.errors.any?
    #error_explanation
      %h2= "#{pluralize(@redcorporativa_interface.errors.count, "error")} prohibited this redcorporativa_interface from being saved:"
      %ul
        - @redcorporativa_interface.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :device
    = f.text_field :device
  .field
    = f.label :port
    = f.text_field :port
  .field
    = f.label :servicio
    = f.text_field :servicio
  .field
    = f.label :gbpsrx
    = f.text_field :gbpsrx
  .field
    = f.label :gbpstx
    = f.text_field :gbpstx
  .field
    = f.label :bps_max
    = f.text_field :bps_max
  .field
    = f.label :statustx
    = f.text_field :statustx
  .field
    = f.label :statusrx
    = f.text_field :statusrx
  .field
    = f.label :status
    = f.text_field :status
  .field
    = f.label :last_bps_max
    = f.text_field :last_bps_max
  .field
    = f.label :last_status
    = f.text_field :last_status
  .field
    = f.label :crecimiento_rx
    = f.text_field :crecimiento_rx
  .field
    = f.label :crecimiento_tx
    = f.text_field :crecimiento_tx
  .field
    = f.label :egressRate
    = f.text_field :egressRate
  .field
    = f.label :time
    = f.date_select :time
  .field
    = f.label :comment
    = f.text_area :comment
  .field
    = f.label :deviceindex
    = f.text_field :deviceindex
  .actions
    = f.submit 'Save'
